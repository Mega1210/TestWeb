@model TestWeb.ViewModels.ListMovies
@{
    ViewBag.Title = "ListMovies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>


    <table>
        <tr>
            <th>
                Movie Name
            </th>
        </tr>
        @foreach (var movie in Model.listOfMovies)
        {

            <tr>
                <td>
                    @movie.Name
                </td>
            </tr>



        }
    </table>





